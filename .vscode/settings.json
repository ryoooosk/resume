{
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": "explicit"
  },
  "textlint.autoFixOnSave": true,
  "textlint.run": "onSave",
  "markdownlint.run": "onSave",
  "files.associations": {
    "*.md": "markdown"
  },
  "editor.formatOnSave": true,
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  }
}